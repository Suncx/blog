<!DOCTYPE html>
<html>
  <head>
    <title>Suncx</title>
    <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
    <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <%= render "share/top" %>
    <div class="container">
      <div class="row"> 
        <div class="span9">
          <div class="left">
            <%= yield %>
          </div>
        </div>
        <div class="span3 offset0">
          <div class="right">
            <%= render "share/right_tags"%>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>
